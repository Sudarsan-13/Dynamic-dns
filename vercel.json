{
  "builds": [{
    "src": "./app.py",
    "use": "@vercel/python",
    "config": {
      "runtime": "python3.10",
      "buildCommand": "pip install --upgrade pip"
    }
  }],
  "routes": [{ "src": "/(.*)", "dest": "./app.py" }],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.10"
    }
  }
}
